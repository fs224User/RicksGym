        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kundenuebersicht.View"
        mc:Ignorable="d"
        Title="MemberDatails" Height="450" Width="800">
    <Grid Background="#FF9D9D9D">
        <Label Content="Name:" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Width="71" FontWeight="Bold"/>
        <Label x:Name="txt_name" Content="" HorizontalAlignment="Left" Margin="86,41,0,0" VerticalAlignment="Top" Width="230"/>
