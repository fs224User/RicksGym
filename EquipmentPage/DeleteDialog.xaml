        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EquipmentPage"
        mc:Ignorable="d"
        Title="DeleteDialog" Height="250" Width="350">
    <Grid>
        <Label x:Name="label_DeleteQuestion" Content="Label" HorizontalAlignment="Center" Height="76" Margin="0,10,0,0" VerticalAlignment="Top" Width="230"/>
        <Button x:Name="btn_AcceptDelete" Content="Yes" HorizontalAlignment="Center" Height="23" Margin="0,101,0,0" VerticalAlignment="Top" Width="106" Click="btn_AcceptDelete_Click"/>

    </Grid>
</Window>
